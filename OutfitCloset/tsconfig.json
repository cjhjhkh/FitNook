{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["esnext", "dom"],
    "types": ["@dcloudio/types", "miniprogram-api-typings"],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "exclude": ["node_modules", "unpackage"]
}